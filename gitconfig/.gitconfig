[log]
    date = local
    graph
[alias]
    lg = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit
    xl = log --branches --graph  --oneline --simplify-by-decoration
    up = switch
[diff]
    tool = vimdiff
[diftool]
    prompt = false
[color]
    ui = auto
[user]
    name = Yuchen Ying
    email = github.com@yegle.net
[push]
	default = current
[core]
    pager = "less -F -X"
